
<div class="project-container" >
	<div class="proj project-navigation">
   	
   	<a href="#" class="home-link">
   	<span class="glyphicon glyphicon-remove" aria-hidden="true" style="font-size:1.5em"></span>
   	</a>
	
	</div>
	<div class="proj project-content" >
		<header class="project-header" data-project-id={{project._id}}>
			<div class="project-title" id="projectView-title">
				{{project.title}}
			</div>
			<div class="project-description" id="projectView-description">
				{{project.description}}
			</div>
			<a href="#" id="myButton-upvote"><span id="projectView-upvotes">{{project.upvoterUsernames.length}}</span> | Upvote</a>
            <a href="#" id="favorite-button">Add to favorites</a>
		</header>
		<div class="project-body">
			<div class="project-body-left">
				<div class="project-media">
					{{#each project.imageLinks}}
						<img src={{this}} alt="image" height="150px">
					{{/each}}
				</div>
				<div class="project-discussion">
					Discussion
					<div class="project-discussion-post">
						Hey My name is George
						<div class="project-discussion-comment">
							<div class="post-content discussion-comment-user">
								George Ezenna
							</div>
							<div class="post-content discussion-comment-body">
							Is that so
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="project-body-right"></div>
		</div>
	</div>
	</div>

